create new user
<%=simple_form_for @user, url: admin_users_path(@user) do |f| %>
  <%=f.input :email%>
  <%=f.input :password%>
  <%=f.input :password_confirmation%>
  <%=f.input :role, collection: ["customer", "merchant", "employee", "admin"]%>

  <%=f.button :submit, "submit", class:["btn-sm", "btn-w-m btn-primary"] %>
  <%=link_to "back", admin_users_path%>

<%end%>
