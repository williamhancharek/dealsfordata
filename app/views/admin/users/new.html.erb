<%=simple_form_for @user, url: admin_users_path(@user) do |f| %>
  <%=f.input :email, presence: true%>
  <%=f.input :password, presence: true%>
  <%=f.input :password_confirmation, presence: true%>
  <%=f.input :role, collection: ["customer", "merchant", "employee", "admin"], presence: true%>

  <%=f.button :submit, "submit", class:["btn-sm", "btn-w-m btn-primary"] %>
  <%=link_to "back", admin_users_path%>

<%end%>
