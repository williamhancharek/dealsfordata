customer new offer page

<%= simple_form_for(@offer, method: :post, url: customer_offers_path) do |f| %>
<%=f.input :link%>
<%=f.input :commentary%>
<%=f.input :tags%>
<%=f.input :retail_price%>
<%=f.input :current_price%>
<%=f.input :campaign_id, as: :hidden%>
<%=f.input :box_id, as: :hidden%>
<%=f.button :submit%>
<% end %>
