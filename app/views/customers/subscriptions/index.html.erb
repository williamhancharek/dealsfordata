<%=render partial: "partials/crumb", locals: {active: "Subscriptions", passive: [link_to("Home", customer_boxes_path), link_to(@box.name, customer_box_path(@box))]}%>
<div class="row">
  <%=render partial: "partials/box_tabs", locals: {box: @box, active: "following"}%>

  <div class="col-10">
    customer box subscriptions index Subscriptions for Box #<%=@box.id%>
    <table>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>address</th>
        <th>search terms</th>
        <th>key words</th>
      </tr>
      <%=render partial: "box", collection: @subscriptions, as: :box%>
    </table>
  </div>
</div>
