<tr>
  <td><%=box.id%></td>
  <td><%=box.name%></td>
  <td><%=box.gender%></td>
  <td><%=box.search_terms%></td>
  <td><%=box.keywords%></td>
  <td><%=box.modifiers%></td>
  <td><%=box.updated_at%></td>
  <td><%=box.address%></td>
  <%if (box.approved_offers.count > 1)%>
    <td><%=link_to "needs approval", employee_box_offers_path(box)%></td>
  <%else%>
    <td><%=link_to "offers", employee_box_offers_path(box)%></td>
  <%end%>
</tr>
